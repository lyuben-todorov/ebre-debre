    "@types/express": "^4.17.6",
    "@types/ioredis": "^4.16.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.155",
    "@types/mongoose": "^5.7.24",
    "@types/morgan": "^1.9.1",
