    "express": "^4.17.1",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.19",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1"
  }
