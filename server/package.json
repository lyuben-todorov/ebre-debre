    "concurrently": "^5.2.0",
    "eslint": "^7.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chess.js": "^0.10.1",
    "@types/cookie-parser": "^1.4.2",
